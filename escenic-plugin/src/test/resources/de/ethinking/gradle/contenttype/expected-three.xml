<?xml version='1.0' encoding='UTF-8'?><content-types xmlns="http://xmlns.escenic.com/2008/content-type" version="4">
  <ui:group xmlns:ui="http://xmlns.escenic.com/2008/interface-hints" name="widgets_alexanderplatz">
    <ui:label>Alexanderplatz UI group</ui:label>
  </ui:group>
  <ui:group xmlns:ui="http://xmlns.escenic.com/2008/interface-hints" name="widgets_hackeshermarkt">
    <ui:label>Hackeshermarkt UI group</ui:label>
    <ui:ref-content-type name="widget_alice"/>
