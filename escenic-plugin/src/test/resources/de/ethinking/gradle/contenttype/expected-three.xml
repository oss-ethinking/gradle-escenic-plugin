  <ui:group xmlns:ui="http://xmlns.escenic.com/2008/interface-hints" name="widgets_nikolaiviertel">
    <ui:label>Nikolaiviertel UI group</ui:label>
    <ui:ref-content-type name="widget_alice"/>
